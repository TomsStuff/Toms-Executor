-- Display a notification to the user
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Tom's ESP + Aimbot",
    Text = "Successfully Loaded!",
    Duration = 5,  -- Notification will be displayed for 5 seconds
    Button1 = "OK"
})

-- Load the ESP script from the external URL
loadstring(game:HttpGet('https://raw.githubusercontent.com/Lucasfin000/SpaceHub/main/UESP'))()

Wait(2)

loadstring(game:HttpGet('https://raw.githubusercontent.com/TomsStuff/Toms-Executor/main/Aimbot'))()
